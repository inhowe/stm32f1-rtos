Nvoid  Mem_Clr (void        *pmem,
N               CPU_SIZE_T   size)
N{
N    Mem_Set((void     *)pmem,
N            (CPU_INT08U)0,                                      /* See Note #2.                                         */
N            (CPU_SIZE_T)size);
N}
