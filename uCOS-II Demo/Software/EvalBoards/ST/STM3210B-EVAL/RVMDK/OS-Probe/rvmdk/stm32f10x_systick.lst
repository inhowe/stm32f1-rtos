N/*******************************************************************************
N* Function Name  : SysTick_CLKSourceConfig
N* Description    : Configures the SysTick clock source.
N* Input          : - SysTick_CLKSource: specifies the SysTick clock source.
N*                    This parameter can be one of the following values:
N*                       - SysTick_CLKSource_HCLK_Div8: AHB clock divided by 8
N*                         selected as SysTick clock source.
N*                       - SysTick_CLKSource_HCLK: AHB clock selected as
N*                         SysTick clock source.
N* Output         : None
N* Return         : None
N*******************************************************************************/
Nvoid SysTick_CLKSourceConfig(u32 SysTick_CLKSource)
N{
N  /* Check the parameters */
N  assert_param(IS_SYSTICK_CLK_SOURCE(SysTick_CLKSource));
X  ((void)0);
N
N  if (SysTick_CLKSource == SysTick_CLKSource_HCLK)
X  if (SysTick_CLKSource == ((u32)0x00000004))
N  {
N    SysTick->CTRL |= SysTick_CLKSource_HCLK;
X    ((SysTick_TypeDef *) (((u32)0xE000E000) + 0x0010))->CTRL |= ((u32)0x00000004);
N  }
N  else
N  {
N    SysTick->CTRL &= SysTick_CLKSource_HCLK_Div8;
X    ((SysTick_TypeDef *) (((u32)0xE000E000) + 0x0010))->CTRL &= ((u32)0xFFFFFFFB);
N  }
N}
N
N/*******************************************************************************
N* Function Name  : SysTick_SetReload
N* Description    : Sets SysTick Reload value.
N* Input          : - Reload: SysTick Reload new value.
N*                    This parameter must be a number between 1 and 0xFFFFFF.
N* Output         : None
N* Return         : None
N*******************************************************************************/
Nvoid SysTick_SetReload(u32 Reload)
N{
N  /* Check the parameters */
N  assert_param(IS_SYSTICK_RELOAD(Reload));
X  ((void)0);
N
N  SysTick->LOAD = Reload;
X  ((SysTick_TypeDef *) (((u32)0xE000E000) + 0x0010))->LOAD = Reload;
N}
