N/*******************************************************************************
N* Function Name  : SysTick_ITConfig
N* Description    : Enables or disables the SysTick Interrupt.
N* Input          : - NewState: new state of the SysTick Interrupt.
N*                    This parameter can be: ENABLE or DISABLE.
N* Output         : None
N* Return         : None
N*******************************************************************************/
Nvoid SysTick_ITConfig(FunctionalState NewState)
N{
N  /* Check the parameters */
N  assert_param(IS_FUNCTIONAL_STATE(NewState));
X  ((void)0);
N
N  if (NewState != DISABLE)
N  {
N    SysTick->CTRL |= CTRL_TICKINT_Set;
X    ((SysTick_TypeDef *) (((u32)0xE000E000) + 0x0010))->CTRL |= ((u32)0x00000002);
N  }
N  else
N  {
N    SysTick->CTRL &= CTRL_TICKINT_Reset;
X    ((SysTick_TypeDef *) (((u32)0xE000E000) + 0x0010))->CTRL &= ((u32)0xFFFFFFFD);
N  }
N}
