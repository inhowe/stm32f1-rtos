N/*******************************************************************************
N* Function Name  : SysTick_CounterCmd
N* Description    : Enables or disables the SysTick counter.
N* Input          : - SysTick_Counter: new state of the SysTick counter.
N*                    This parameter can be one of the following values:
N*                       - SysTick_Counter_Disable: Disable counter
N*                       - SysTick_Counter_Enable: Enable counter
N*                       - SysTick_Counter_Clear: Clear counter value to 0
N* Output         : None
N* Return         : None
N*******************************************************************************/
Nvoid SysTick_CounterCmd(u32 SysTick_Counter)
N{
N  /* Check the parameters */
N  assert_param(IS_SYSTICK_COUNTER(SysTick_Counter));
X  ((void)0);
N
N  if (SysTick_Counter == SysTick_Counter_Enable)
X  if (SysTick_Counter == ((u32)0x00000001))
N  {
N    SysTick->CTRL |= SysTick_Counter_Enable;
X    ((SysTick_TypeDef *) (((u32)0xE000E000) + 0x0010))->CTRL |= ((u32)0x00000001);
N  }
N  else if (SysTick_Counter == SysTick_Counter_Disable) 
X  else if (SysTick_Counter == ((u32)0xFFFFFFFE)) 
N  {
N    SysTick->CTRL &= SysTick_Counter_Disable;
X    ((SysTick_TypeDef *) (((u32)0xE000E000) + 0x0010))->CTRL &= ((u32)0xFFFFFFFE);
N  }
N  else /* SysTick_Counter == SysTick_Counter_Clear */
N  {
N    SysTick->VAL = SysTick_Counter_Clear;
X    ((SysTick_TypeDef *) (((u32)0xE000E000) + 0x0010))->VAL = ((u32)0x00000000);
N  }    
N}
